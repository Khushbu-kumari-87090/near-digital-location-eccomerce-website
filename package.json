    "colors": "^1.4.0",
    "dotenv": "^8.6.0",
    "express": "^4.18.2",
    "express-async-handler": "^1.2.0",
    "jsonwebtoken": "^8.5.1",
    "mongoose": "^5.13.16",
    "morgan": "^1.10.0",
    "multer": "^1.4.2",
    "react-google-login": "^5.2.2",
    "webpack": "^5.76.1",
    "yarn": "^1.22.19"
  },
  "devDependencies": {
    "concurrently": "^5.3.0",
    "nodemon": "^2.0.21"
  }
